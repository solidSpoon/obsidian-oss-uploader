{
	"name": "obsidian-aliyun-oss-uploader",
	"version": "1.0.0",
	"description": "An Obsidian plugin for uploading images to Aliyun OSS",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"aliyun",
		"oss",
		"image"
	],
	"author": "solidSpoon",
	"license": "MIT",
	"dependencies": {
		"browser-image-compression": "^2.0.2"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
